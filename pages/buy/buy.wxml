    <view class="listitem01">
        <view catchtap="tap_caac7aaf" class="c-minheight bottomGrayboder wx-view_pOFBSt" data-buy_id="{{item.id}}" wx:for="{{buys}}">
            <image class="stick" src="/resources/zhidiang.png" wx:if="{{item.position==2}}"></image>
            <navigator class="listitem-navigator" hoverClass="navigator-hover">
                <view class="wx-image_tBcdCT2">
                    <image class="img" mode="aspectFill" src="{{item.icon}}"></image>
                </view>
                <view class="wx-view_cPj0ok2">
                    <view class="c-minheight wx-view_gmUz5O">
                        <text class="c-block c-ellipse wx-text_TvHPc92">{{item.fang_name}}</text>
                    </view>
                    <view class="c-minheight wx-view_qSNbVJ">
                        <text class="c-block wx-text_dFoG8W">{{item.district}}</text>
                        <text class="c-block wx-text_BM3ABC">{{item.xiaoqu_name}}</text>
                    </view>
                    <view class="c-minheight wx-view_eSPd6T">
                        <view class="c-minheight wx-view_ehjyjk">
                            <text class="c-block wx-text_UdF2my">{{item.huxing}}</text>
                            <text class="c-block wx-text_rg3ugt">{{item.size}}㎡</text>
                        </view>
                        <view class="c-minheight wx-view_8bRi3G" wx:if="{{item.total!=0}}">
                            <text class="c-block wx-text_q7eeg3">{{item.total}}</text>
                            <text class="c-block wx-text_HQhFyQ">万元</text>
                        </view>
                        <view class="c-minheight wx-view_8bRi3G" wx:if="{{item.total==0}}">
                            <text class="c-block wx-text_q3eeg7">面议</text>
                            <text class="c-block wx-text_HQhFyQ">万元</text>
