        <view class="search-box center">
            <input class="search_v" name="search_value" placeholder="请输入房屋名或小区名" type="text" value="{{search_value}}"></input>
            <button class="search_b center" formType="submit">
                <image class="search-img" src="/resources/search.png"></image>
            </button>
        </view>
    </form>
